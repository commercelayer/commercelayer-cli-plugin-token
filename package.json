{
  "name": "@commercelayer/cli-plugin-token",
  "description": "Commerce Layer CLI Token plugin",
  "version": "2.0.0-beta.0",
  "author": "Pierluigi Viti <pierluigi@commercelayer.io>",
  "bin": {
    "cl-token": "./bin/run"
  },
  "bugs": "https://github.com/commercelayer/commercelayer-cli-plugin-token/issues",
  "devDependencies": {
    "@oclif/dev-cli": "git+https://github.com/pviti/dev-cli.git",
    "@oclif/plugin-help": "^3.2.10",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.22",
    "@types/inquirer": "^8.1.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/mocha": "^5.2.7",
    "@types/node": "^16.11.0",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "chai": "^4.3.4",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "eslint-config-xo-space": "^0.20.0",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-unicorn": "^6.0.1",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": ">=12.20"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/commercelayer/commercelayer-cli-plugin-token",
  "keywords": [
    "oclif",
    "ecommerce",
    "cli",
    "commercelayer"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "cl-token",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "plugins": [],
    "topics": {
      "token": {
        "description": "Manage Commerce Layer access tokens",
        "hidden": true
      }
    },
    "repositoryPrefix": "<%- repo %>/blob/main/<%- commandPath %>"
  },
  "repository": "commercelayer/commercelayer-cli-plugin-token",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && npm run readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "readme": "oclif-dev readme --plugin --bin=commercelayer && git add README.md"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@commercelayer/cli-core": "^1.0.0",
    "@commercelayer/js-auth": "^2.2.4",
    "@commercelayer/sdk": "^2.3.12",
    "@oclif/command": "^1.8.4",
    "@oclif/config": "^1.17.1",
    "chalk": "^4.1.2",
    "inquirer": "^8.2.0",
    "jsonapi-typescript": "^0.1.3",
    "tslib": "^2.3.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
